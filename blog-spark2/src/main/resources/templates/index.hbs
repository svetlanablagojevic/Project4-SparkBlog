{{#partial "content"}}
    <div class="entry-list">
        {{#each blogEntries}}
            <article>
                <h2><a href="/index/{{ this.slug }}">{{ this.title }}</a></h2>
                <time datetime="{{ this.date }}">{{ this.timeString }}</time>
            </article>
        {{/each}}
    </div>
{{/partial}}

{{> base.hbs}}